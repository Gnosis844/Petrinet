<?xml version="1.0" encoding="UTF-8"?>
<pnml xmlns="http://www.pnml.org/version-2009/grammar/pnml">
  <net id="self_loop_place" type="http://www.pnml.org/version-2009/grammar/ptnet">
    <!-- Invalid: arc from place to place (should be place->transition or transition->place) -->
    <place id="P1">
      <name><text>P1</text></name>
      <initialMarking><text>1</text></initialMarking>
    </place>
    <place id="P2">
      <name><text>P2</text></name>
      <initialMarking><text>0</text></initialMarking>
    </place>
    <arc id="A1" source="P1" target="P2"/>
  </net>
</pnml>
