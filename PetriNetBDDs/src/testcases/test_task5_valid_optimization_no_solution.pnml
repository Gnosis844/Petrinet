<?xml version="1.0" encoding="UTF-8"?>
<pnml xmlns="http://www.pnml.org/version-2009/grammar/pnml">
  <net id="optimization_no_solution" type="http://www.pnml.org/version-2009/grammar/ptnet">
    <!-- Optimization with no reachable solution for objective -->
    <!-- P1 -> T1 -> P2, but T1 requires P3 (never marked) -->
    <!-- Only reachable marking is {P1}, so P2 can never be reached -->
    <place id="P1">
      <name><text>P1</text></name>
      <initialMarking><text>1</text></initialMarking>
    </place>
    <place id="P2">
      <name><text>P2</text></name>
      <initialMarking><text>0</text></initialMarking>
    </place>
    <place id="P3">
      <name><text>P3</text></name>
      <initialMarking><text>0</text></initialMarking>
    </place>
    <transition id="T1">
      <name><text>T1</text></name>
    </transition>
    <arc id="A1" source="P1" target="T1"/>
    <arc id="A2" source="P3" target="T1"/>
    <arc id="A3" source="T1" target="P2"/>
  </net>
</pnml>
